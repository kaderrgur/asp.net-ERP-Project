
@{
    ViewBag.Title = "isOrtaklariEkle";
    Layout = "~/Views/admin/layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                <h4>@WebApplication1.Language.Localization.IsOrtagiEkleBaslik</h4>
            </header>
            <div class="panel-body">
                <form role="form" class="form-horizontal tasi-form" action="/admin/isOrtaklariEkle" method="post">
                    <div class="form-group">
                        <label class="col-lg-2 control-label">@WebApplication1.Language.Localization.FirmaAd</label>
                        <div class="col-lg-10">
                            <input type="text" placeholder="" id="f-name" class="form-control" name="FirmaAd">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">@WebApplication1.Language.Localization.Email</label>
                        <div class="col-lg-10">
                            <input type="email" placeholder="" id="email2" class="form-control" name="Email">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">@WebApplication1.Language.Localization.Telefon</label>
                        <div class="col-lg-10">
                            <input type="text" placeholder="" id="f-name" class="form-control" name="Gsm">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">@WebApplication1.Language.Localization.Adres</label>
                        <div class="col-lg-10">
                            <input type="text" placeholder="" id="f-name" class="form-control" name="Adres">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-offset-2 col-lg-10">
                            <button class="btn btn-danger" type="submit">@WebApplication1.Language.Localization.Kaydet</button>
                        </div>
                    </div>
                </form>
            </div>
        </section>
    </div>
</div>

